                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
    dp.Ngaydat AS NgayDung,
    ch.Mota AS CaHoc,
    p.Tenphong AS PhongSuDung,
    CASE 
        WHEN dp.MaDuyet IS NULL THEN N'Chờ duyệt'
        ELSE lsd.Trangthai
    END AS Trangthai
FROM 
    TB_LichsuDP dp
JOIN 
    TB_Cahoc ch ON dp.Maca = ch.Maca
JOIN 
    TB_Phong p ON dp.Maphong = p.Maphong
LEFT JOIN 
    TB_Lichsuduyet lsd ON dp.MaDuyet = lsd.Malichsu
ORDER BY 
    dp.Ngaydat, ch.Maca
</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
